[
    {
        "name": "core/post-template",
        "attributes": {
            "className": "meros-carousel-dynamic-template",
            "layout": {
                "type": "default",
                "inherit": false
            },
            "style": {
                "spacing": {
                    "blockGap": "0px"
                }
            }
        },
        "innerBlocks": [
            {
                "name": "core/group",
                "attributes": {
                    "tagName": "div",
                    "layout": {
                        "type": "constrained",
                        "justifyContent": "center"
                    },
                    "style": {
                        "color": {
                            "text": "#FFFFFF"
                        }
                    }
                },
                "innerBlocks": [
                    {
                        "name": "core/post-title",
                        "attributes": {
                            "level": 2,
                            "isLink": true,
                            "rel": "",
                            "linkTarget": "_self"
                        }
                    },
                    {
                        "name": "core/post-excerpt",
                        "attributes": {
                            "showMoreOnNewLine": true,
                            "excerptLength": 55
                        }
                    },
                    {
                        "name": "core/group",
                        "attributes": {
                            "tagName": "div",
                            "layout": {
                                "type": "constrained",
                                "justifyContent": "left"
                            }
                        },
                        "innerBlocks": [
                            {
                                "name": "core/read-more",
                                "attributes": {
                                    "content": "Read more",
                                    "style": {
                                        "border": {
                                            "radius": "5px"
                                        },
                                        "color": {
                                            "background": "#212121"
                                        },
                                        "spacing": {
                                            "padding": {
                                                "bottom": "var:preset|spacing|20",
                                                "left": "var:preset|spacing|30",
                                                "right": "var:preset|spacing|30",
                                                "top": "var:preset|spacing|20"
                                            }
                                        }
                                    }
                                }
                            }
                        ]
                    }
                ]
            }
        ]
    }
]
